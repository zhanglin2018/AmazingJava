<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>

   <extension
         id="application"
         point="org.eclipse.core.runtime.applications">
      <application>
         <run
               class="gef_turials.Application">
         </run>
      </application>
   </extension>
   <extension
         point="org.eclipse.ui.perspectives">
      <perspective
            class="gef_turials.Perspective"
            fixed="true"
            icon="icons/eclipse64.png"
            id="GEF_Turials.perspective"
            name="RCP Perspective">
      </perspective>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.advantest.gef.core.MyGraphicalEditor"
            contributorClass="com.advantest.gef.core.MyGraphicalEditorActionBarContributor"
            default="true"
            id="com.advantest.gef.core.mygraphicaleditor"
            name="MyGraphicalEditor">
      </editor>
   </extension>

</plugin>
